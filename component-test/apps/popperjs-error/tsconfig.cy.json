{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "sourceMap": false,
    "allowJs": true,
    "outDir": "../../dist/out-tsc",
    "module": "commonjs",
    "types": ["cypress", "node"]
  },
  "include": [
    "cypress.config.ts",
    "**/*.cy.ts",
    "**/*.cy.tsx",
    "**/*.cy.js",
    "**/*.cy.jsx",
    "**/*.d.ts",
    "cypress/*",
    "cypress/**/*"
  ]
}
